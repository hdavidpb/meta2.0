{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/transform-runtime"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@utils": "./src/utils",
          "@pages": "./src/pages",
          "@components": "./src/components",
          "@styles": "./src/styles",
          "@api": "./src/api",
          "@assets": "./src/assets",
          "@hooks": "./src/hooks",
          "@services": "./src/services",
          "@context": "./src/context",
          "@routes": "./src/routes",
          "@redux": "./src/redux"
        }
      }
    ]
  ]
}
